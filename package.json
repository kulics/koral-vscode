{
  "name": "koral-language-support",
  "displayName": "Koral Language Support",
  "description": "Basic syntax highlighting support for the Koral language.",
  "version": "0.0.5",
  "publisher": "kulics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kulics/koral-vscode.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "koral",
    "syntax-highlighting",
    "language"
  ],
  "contributes": {
    "languages": [
      {
        "id": "koral",
        "aliases": [
          "Koral",
          "koral"
        ],
        "extensions": [
          ".koral"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "koral",
        "scopeName": "source.koral",
        "path": "./syntaxes/koral.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1"
  }
}
